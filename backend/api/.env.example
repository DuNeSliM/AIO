# Server Configuration
PORT=8080

# IsThereAnyDeal API
ISTHEREANYDEAL_API_KEY=your_itad_api_key_here

# Keycloak Configuration
# URL of your Keycloak server (without trailing slash)
KEYCLOAK_URL=https://auth.gamedivers.de
# Name of the realm for your application
KEYCLOAK_REALM=gamedivers
# Client ID registered in Keycloak
KEYCLOAK_CLIENT_ID=gamedivers-api
# Client secret for the client (confidential client)
KEYCLOAK_CLIENT_SECRET=your_client_secret_here

# Database Configuration (optional, for user persistence)
# DATABASE_URL=postgres://user:password@localhost:5432/gamedivers?sslmode=disable
